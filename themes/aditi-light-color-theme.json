  "tokenColors": [
    {
      "settings": {
        "foreground": "#303952",
        "background": "#fff"
      }
    },
    {
      "name": "Key",
      "scope": [
        "support.type.property-name.json",
        "support.type.property-name.css",
        "meta.definition.variable",
        "variable.object.property",
        "entity.other.attribute-name.html",
        "variable.other.readwrite",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#1c98bf"
      }
    },
    {
      "name": "Value",
      "scope": [
        "meta.structure.dictionary.value.json",
        "support.constant.property-value.css"
      ],
      "settings": {
        "foreground": "#ea8685"
      }
    },
    {
      "name": "Enclose",
      "scope": ["entity.name.tag.block.any", "meta.tag.block.any.html"],
      "settings": {
        "foreground": "#1d5b79"
      }
    },
    {
      "name": "Boolean, null",
      "scope": ["constant.language.json", "constant.language.null"],
      "settings": {
        "foreground": "#546de5"
      }
    },
    {
      "name": "String",
      "scope": ["string.quoted", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#009688"
      }
    },
    {
      "name": "Type, Keyword",
      "scope": ["storage.type", "keyword.control", "storage.modifier.ts"],
      "settings": {
        "foreground": "#7e7eb9"
      }
    },
    {
      "name": "Block",
      "scope": [
        "meta.block",
        "meta.object.member",
        "entity.name.type.class",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#c57f97"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c1cbd4"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "string.quoted.double.json",
        "punctuation.definition.string.end.json"
      ],
      "settings": {
        "foreground": "#6a89cc"
      }
    },
    {
      "name": "Method",
      "scope": [
        "meta.definition.method",
        "meta.method.declaration",
        "storage.type"
      ],
      "settings": {
        "foreground": "#2a80cc"
      }
    },
    {
      "name": "Typescript type",
      "scope": ["entity.name.type", "support.type.primitive"],
      "settings": {
        "foreground": "#9f8bb9"
      }
    }
  ]
}
